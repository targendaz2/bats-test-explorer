{
  "name": "bats-test-explorer",
  "displayName": "Bats Test Explorer",
  "description": "",
  "version": "0.1.0",
  "publisher": "dgrdev",
  "author": "David Rosenberg",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Testing"
  ],
  "keywords": [
    "bash",
    "bats",
    "bats-core"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/targendaz2/bats-test-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/targendaz2/bats-test-explorer/issues"
  },
  "activationEvents": [
    "workspaceContains:**/*.bats"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bats-test-explorer.installBats",
        "title": "Install 'Bats' to project",
        "category": "Shell Command"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/tmp": "^0.2.3",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "chai": "^4.3.7",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.1",
    "tmp-promise": "^3.0.3",
    "typescript": "^5.1.3",
    "vscode-uri": "^3.0.7"
  }
}
